<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Document</title>
    <link rel="stylesheet" href="style/Layout.css">
    <link rel="stylesheet" href="style/Hamburger.css">
    <link rel="stylesheet" href="style/Youtube-logo.css">
    <link rel="stylesheet" href="style/Searchbar.css">
    <link rel="stylesheet" href="style/User-navigation.css">
    <link rel="stylesheet" href="style/Profile-button.css">
    <link rel="stylesheet" href="style/Header.css">
</head>
<body class="Layout">
    <header class="Layout__header">
        <div class="Header">
            <div class="Header__left">
                <img class="Hamburger"
                     src="https://upload.wikimedia.org/wikipedia/commons/thumb/b/b2/Hamburger_icon.svg/1200px-Hamburger_icon.svg.png">
                <img class="Youtube-logo" src="data:image/jpeg;base64," alt="youtube"/>
                <form action="search" class="Searchbar">
                    <input type="text" class="Searchbar__searchfield" placeholder="Пошук"/>
                    <!--<button class="Searchbar__keyboard"></button>-->
                    <button type="submit" class="Searchbar__searchbutton"><img class="Searchbar__searchicon"
                                                                               src="https://digiday.com/wp-content/themes/digiday/static/images/icon_search.svg"/>
                    </button>
                </form>
            </div>
            <div class="Header__right">
                {{#action-button}}
                    <button type="button" class="User-navigation"><img class="User-navigation__icon"
                                                                                src="{{icon}}"/>
                    </button>
                {{/action-button}}
                <button type="button" class="Profile-button"><img class="Profile-button__icon"
                                                                  src="https://yt3.ggpht.com/-Sf5jwMHGczE/AAAAAAAAAAI/AAAAAAAAAAA/gMmMai3rK4Q/s88-c-k-no-mo-rj-c0xffffff/photo.jpg"/>
                </button>
            </div>
        </div>
    </header>


