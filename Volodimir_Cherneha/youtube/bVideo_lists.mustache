<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="style/Video_lists.css">
</head>
<body>
    <div class="Video-lists">
        {{#video}}
            <section class="Video-lists__section">
                <h6 class="Video-lists__header">{{section-name}}</h6>
                {{#items}}
                    <article class="Video-lists__video">
                        <img class="Video-lists__video-screen" src="{{image}}"/>
                        <p class="Video-lists__description-black">{{description}}</p>
                        <p class="Video-lists__description-grey">{{{footer}}}</p>
                    </article>
                {{/items}}
            </section>
        {{/video}}
    </div>
</body>
</html>