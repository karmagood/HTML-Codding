<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="style/Help.css">
    <link rel="stylesheet" href="style/Copyright-and-policy.css">
    <link rel="stylesheet" href="style/Footer.css">
</head>
<body>
    <div class="Footer">
        <section class="Help">
            <a class="Help__settings" href="https://www.youtube.com/account">Налаштування</a>
            <button type="button" class="Help__reference">Довідка</button>
            <button type="button" class="Help__feedback">Написати відгук</button>
        </section>
        {{#copyright-and-policy}}
            <section class="Copyright-and-policy">
                {{#items}}
                    <a class="Copyright-and-policy__item" href="{{url}}">{{title}}</a>
                {{/items}}
            </section>
        {{/copyright-and-policy}}
        <p class="Footer__copyright-line">© 2017 YouTube, LLC</p>
    </div>
</body>
</html>