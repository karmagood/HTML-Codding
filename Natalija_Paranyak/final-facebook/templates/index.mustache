<header class="main-navigation">
    <a class="main-navigation__item_big" href="https://www.facebook.com/"><img class="main-navigation__item__logo"
                                                                               src="../images/facebook-icon.jpg"
                                                                               height="25"
                                                                               alt="Facebook logo"/></a>
    <input class="main-navigation__input" type="text" placeholder="Пошук"><input class="main-navigation__submit"
                                                                                 type="submit" value="&#xf002">
    <div class="main-navigation__separator"></div>
    <a class="main-navigation__item_big" href="https://www.facebook.com/"><img class="profile-img"
                                                                               src="../images/home-team-3.jpg"
                                                                               alt="Profile photo"></a>
    <a class="main-navigation__item_big" href="#"><p class="main-navigation__paragraph">Наталя</p></a>
    <a class="main-navigation__item_big"><p class="main-navigation__paragraph">Головна </p></a>
    <a class="main-navigation__item main-navigation__item_friend" href="#">Запити на дружбу</a>
    <a class="main-navigation__item main-navigation__item_messenger" href="#">Повідомлення</a>
    <a class="main-navigation__item main-navigation__item_news" href="#">Сповіщення</a>
    <div class="main-navigation__item_liner"></div>
    <a class="main-navigation__item main-navigation__item_help" href="#">Швидка допомога</a>
    <a class="main-navigation__item main-navigation__item_quick" href="#">Швидкий доступ</a>
</header>
<section class="main-container">
    <section class="main-container__nav">
        <nav>
            <section class="main-container__nav__block">

                <a class="main-container__link" href="#">
                    <img class="main-container__img" src="../images/home-team-3.jpg" alt="Profile picture">
                    <p class="main-container__nav__paragraph"> Наталя Параняк</p></a>

                <section>
                    {{#news-messenger}}
                        <a class="main-container__link" href="{{src}}">
                            <img class="main-container__nav__img" src="{{img-src}}" style="{{img-options}}">
                            <p class="main-container__nav__paragraph">{{text}}</p></a>
                    {{/news-messenger}}
                </section>
                <h2 class="main-container__header">Швидкий доступ</h2>
                {{#menu-items}}
                    <a class="main-container__link" href="{{menu-src}}">
                        <img src="{{img-src}}" alt="Page 1" class="main-container__img">
                        <p class="main-container__nav__paragraph">{{name-item}}</p></a>
                {{/menu-items}}
                <h2 class="main-container__header">Огляд</h2>
                <section>
                    {{#interesting-things}}
                        <a class="main-container__link" href="{{src}}">
                            <img class="main-container__nav__img" src="{{img-src}}" style="{{img-options}}">
                            <p class="main-container__nav__paragraph">{{text}}</p></a>
                    {{/interesting-things}}
                </section>
            </section>

            <aside class="main-container__creation">
                <h2 class="main-container__header">Створити</h2>
                <a class="main-container__creation__link" href="https://www.facebook.com/"><p>Реклама</p></a>
                <a class="main-container__creation__link" href="https://www.facebook.com/"><p>Сторінки</p></a>
                <a class="main-container__creation__link" href="https://www.facebook.com/"><p>Група</p></a>
                <a class="main-container__creation__link" href="https://www.facebook.com/"><p>Подія</p></a>
            </aside>
        </nav>
    </section>
    <section class=" main-container__feeds">
        <div class="main-container__feeds__new">
            <form id="create-post">
                <div class="main-container__feeds__new__actions main-container__feeds__new__actions_new">

                    <button class=" main-container__feeds__button main-container__feeds__new__actions_create"><i
                            class=" main-container__feeds__icons create-post"></i>Створити допис
                    </button>
                    <button class="main-container__feeds__button main-container__feeds__new__actions_album"><i
                            class="main-container__feeds__icons album-post"></i>Альбом зі світлинами/відео
                    </button>
                    <button class="main-container__feeds__button main-container__feeds__new__actions_video"><i
                            class="main-container__feeds__icons video-post"></i>Відео наживо
                    </button>
                </div>
                <div class="main-container__feeds__on-mind">
                    <img class="main-container__feeds__new__on-mind__img" src="images/home-image-1.jpg" height="35">
                    <textarea class="main-container__feeds__new__on-mind__input"
                              placeholder="Що у Вас на думці, Наталя?" name="description"></textarea>
                </div>
                <hr class="main-container__feeds__new__separator">
                <section class="main-container__feeds__new__extend">
                    <div class="main-container__feeds__new__actions">
                        <button class="main-container__feeds__button main-container__feeds__new__actions_photo"><i
                                class="main-container__feeds__icons photos-post "></i>Світлини/Відео
                        </button>
                        <button class="main-container__feeds__button main-container__feeds__new__actions_emotions"><i
                                class="main-container__feeds__icons emotions-post"></i> Почуття або дії
                        </button>
                        <button class="main-container__feeds__button main-container__feeds__new__actions_more"><i
                                class="main-container__feeds__icons more-post"></i></button>
                    </div>
                </section>
                <section class="main-container__feeds__new__publish">
                    <div class="main-container__feeds__new__actions main-container__feeds__new__actions_publish">
                        <button class="main-container__feeds__button main-container__feeds__new__actions_chose"><i
                                class="main-container__feeds__icons public-post"></i>Публічно
                        </button>
                        <button class="main-container__feeds__button main-container__feeds__new__actions_post">
                            Опублікувати
                        </button>
                    </div>
                </section>
            </form>
        </div>
        <div id="news-feed">
            {{#posts}}
                <section class="main-container__feeds__item">
                    <a href="{{profile-url}}"><img class="main-container__feeds__img" src="{{profile-picture-url}}"
                                                   alt="Profile picture" width="55"
                                                   height="55"></a>
                    <div style="display: inline-block">
                        <h2 class="main-container__feeds__header main-container__feeds__header_main"
                        ><a href="{{profile-url}}"
                            class="main-container__quick__link_more">{{profile-name}}</a>
                            <span class="main-container__feeds__header_action">{{user-action}}</span></h2>
                        <h2 class="main-container__feeds__header main-container__feeds__header_small"><a
                                href="{{extra-info-url}}"
                                style="display: inline;">{{time-info}}
                            тому</a><a href="#" style="display: inline;">
                            {{extra-paragraph}}</a> <i class="fa fa-globe main-container__feeds__icon"></i>
                        </h2>

                    </div>


                    <article class="main-container__feeds__text">{{main-post-info}}
                        <span class="main-container__feeds__text_tag">{{tags}}</span>
                    </article>

                    <img class="main-container__feeds__image" src="{{post-images-url}}" width="400"
                    >

                    <section class="main-container__feeds__actions">
                        <button class="main-container__feeds__actions__button" type="submit"><i
                                class="fa fa-thumbs-o-up main-container__feeds__actions__button_icon"
                        ></i>Подобається
                        </button>
                        <button class="main-container__feeds__actions__button" type="submit"><i
                                class="fa fa-comments main-container__feeds__actions__button_icon"
                        ></i>Коментувати
                        </button>
                        <button class="main-container__feeds__actions__button" type="submit"><i
                                class="fa fa-share main-container__feeds__actions__button_icon"
                        ></i>Поширити
                        </button>
                    </section>


                </section>
            {{/posts}}
        </div>
    </section>

    <section class=" main-container__quick">
        <div>
            <section class="main-container__quick__item">
                <a href="#" class="main-container__quick__link">
                    <img class="main-container__quick__link_event" src="../images/gift.png" alt="gift" width="25">
                    <p class="main-container__quick__link_more">4 запрошення на
                        подіїї</p></a>
                <a href="#" class="main-container__quick__link">
                    <img class="main-container__quick__link_birthday" src="../images/gift.png" alt="gift" width="25">
                    <p>Сьогодні день народження у <span class="main-container__quick__link_more">Віктора</span>
                    </p></a>
            </section>

            <section class="main-container__quick__friends">
                <h2 class="main-container__quick__header">
                    Запити на дружбу</h2>
                <a href="#"
                   class="main-container__quick__friends__link">Переглянути
                    більше</a>
                {{#friends-request}}
                    <section class="main-container__quick__friends__item">
                        <a href="{{friend-src}}"> <img class="main-container__quick__friends__item__img"
                                                       src="{{friend-image-src}}" height="50" width="50"
                                                       alt="Friend name"></a>
                        <a href="#">
                            <h3 class="main-container__quick__friends__item__header">{{friend-name}}</h3>
                        </a>
                        <p class="main-container__quick__friends__item__par">{{common-friends-count}} спільних
                            друга</p>
                        <button class="main-container__quick__friends__item__button" type="submit">
                            Підтвердити друга
                        </button>
                    </section>
                {{/friends-request}}


            </section>
            <section class="main-container__quick__item__recomendation">
                <h2 class="main-container__quick__header">
                    Рекомендовані сторінки</h2>
                <a href="#" class="main-container__quick__friends__link">Переглянути
                    всі</a>
                <section class="main-container__quick__item__recomendation__text">
                    <a href="#"> <img class="main-container__quick__item__recomendation__img"
                                      src="../images/headerimg9.jpg" width="290" alt="Main page image"
                    ></a>
                    <a href="#">
                        <h3 class="main-container__quick__item__recomendation__header">
                            Блог Л.М.</h3>
                    </a>
                    <p class="main-container__quick__item__recomendation__par">2345 вподобань</p>
                    <button type="submit" class="main-container__quick__friends__item__button">Уподобати сторінку
                    </button>
                </section>
            </section>

        </div>
    </section>

</section>


<section class=" main-container__actions">

    <div>
        <section class="main-container__actions__games">
            <h2 class="main-container__quick__header">
                ВАШІ ІГРИ</h2>
            <a href="#" class="main-container__quick__friends__link">Ще</a>
            <section class="main-container__actions__item">
                <section>
                    <div class="main-container__actions__item__dialog">
                        <a href="#"> <img class="main-container__actions__item__dialog__img"
                                          src="../images/html5_doctor.jpg" height="45" width="45" alt="Your game"
                        ></a>
                        <a href="#" class="main-container__actions__item--close"></a>
                    </div>
                </section>
                <section>
                    <div class="main-container__actions__item__dialog">
                        <a href="#"> <img class="main-container__actions__item__dialog__img"
                                          src="../images/headerimg9.jpg" height="45" width="45" alt="Your game"
                        ></a>
                        <a href="#" class="main-container__actions__item--close"></a>
                    </div>
                </section>
                <section>
                    <div class="main-container__actions__item__dialog">
                        <a href="#"> <img class="main-container__actions__item__dialog__img"
                                          src="../images/facebook_logo.png" height="45" width="45" alt="Your game"
                        ></a>
                        <a href="#" class="main-container__actions__item--close"></a>
                    </div>
                </section>
            </section>
        </section>
        <hr class="main-container__action__separator">
        <section class="main-container__actions__friends__feeds">
            {{#friends-feeds}}
                <article class="main-container__actions__friends__feeds__item">
                    <a href="{{friend-src}}">
                        <img class="main-container__actions__friends__img" src="{{friend-image-src}}" height="40"
                             width="40" alt="Profile img"
                        >

                        <p class="main-container__actions__friends__par"><span
                                class="main-container__actions__friends__par_name">{{friend-name}}</span>
                            {{action}}</p>
                    </a>
                </article>
                <hr class="main-container__action__separator">
            {{/friends-feeds}}

        </section>
        <section class="main-container__actions__contacts">
            {{#contacts}}
                <section class="main-container__actions__contacts__item"><a href="{{friend-src}}"> <img
                        class="main-container__actions__contacts__img" src="{{friend-image-src}}" height="40" width="40"
                        alt="Profile img">
                    <p class="main-container__actions__contacts__present">{{friend-name}}</p></a>
                    <p class="main-container__actions__contacts__present"><i
                            class="fa fa-circle main-container__actions__contacts__present_active"
                    ></i></p>
                </section>
            {{/contacts}}


        </section>
        <section class="main-container__actions__search">
            <input class="main-container__actions__input" type="text" placeholder="Пошук">
            <a class="main-container__actions__search__icon main-container__actions__search__icon_setting" href="#">Налаштування</a>
            <a class="main-container__actions__search__icon main-container__actions__search__icon_new" href="#">Нове
                повідомлення</a>
            <a class="main-container__actions__search__icon main-container__actions__search__icon_group" href="#">Створити
                нову групу</a>
        </section>
    </div>
</section>
